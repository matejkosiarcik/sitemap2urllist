{
  "name": "sitemap2urllist",
  "version": "0.2.1",
  "license": "MIT",
  "description": "Simple sitemap.xml to urllist.txt converter (library and cli)",
  "homepage": "https://github.com/matejkosiarcik/sitemap2urllist#readme",
  "keywords": [
    "sitemap",
    "sitemap.xml",
    "urllist",
    "urllist.txt",
    "converter",
    "SEO"
  ],
  "bugs": {
    "url": "https://github.com/matejkosiarcik/sitemap2urllist/issues"
  },
  "author": {
    "name": "Matej Košiarčik",
    "email": "matej.kosiarcik@gmail.com",
    "url": "https://matejkosiarcik.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/matejkosiarcik/sitemap2urllist.git"
  },
  "engines": {
    "node": ">=14"
  },
  "main": "dist/lib/lib.js",
  "types": "types/lib/lib.d.ts",
  "bin": {
    "sitemap2urllist": "bin/main.js"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "bin/",
    "dist/",
    "types/lib/"
  ],
  "scripts": {
    "build": "tsc",
    "test": "jest --runInBand",
    "lint": "run-p lint:*",
    "lint:tsc": "tsc --noEmit",
    "lint:es": "eslint --max-warnings 0 \"**/*.{ts,js}\"",
    "lint:prettier": "prettier --list-different '**/*.{yml,yaml,json,html,htm,xhtml,css,scss,sass,md}'",
    "fmt": "run-p fmt:*",
    "fmt:es": "eslint --fix \"**/*.{ts,js}\"",
    "fmt:prettier": "prettier --write '**/*.{yml,yaml,json,html,htm,xhtml,css,scss,sass,md}'"
  },
  "dependencies": {
    "node-fetch": "^2.0.0",
    "xml-js": "^1.6.0",
    "yargs": "^17.0.0"
  },
  "devDependencies": {
    "@types/jest": "^26.0.20",
    "@types/node": "^15.0.0",
    "@types/node-fetch": "^2.5.8",
    "@types/yargs": "^16.0.0",
    "@typescript-eslint/eslint-plugin": "^4.20.0",
    "@typescript-eslint/parser": "^4.20.0",
    "eslint": "^7.23.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-import": "^2.22.1",
    "jest": "^26.6.3",
    "node-prune": "^1.0.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.2.1",
    "ts-jest": "^26.5.3",
    "typescript": "^4.2.3"
  }
}
