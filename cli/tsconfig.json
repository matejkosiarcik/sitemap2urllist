{
  "compilerOptions": {
    "outDir": "build",
    "target": "es5",
    "lib": [
      "esnext"
    ],
    "moduleResolution": "node",
    "strict": true,
    "noFallthroughCasesInSwitch": true,
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true
  },
  "include": [
    "./src/*",
    "main.js"
  ]
}
